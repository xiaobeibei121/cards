.container{
  padding-bottom: 80rpx;
  background-color: #fff;
}

.time-view{
  position: relative;
  z-index: 2;
  height: 120rpx;
  line-height: 120rpx;
  font-size: 50rpx;
  padding-left: 30rpx;
  font-weight: bold;
  color: #666;
  padding-top: 30rpx;
}

.save-view{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  color: #fff;
  background-color: rgb(0, 155, 140);
  font-size: 32rpx;
  text-align: center;
}

.qr-view{
  padding-bottom: 50rpx;
  text-align: center;
}

.qr-icon{
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  margin-bottom: 20rpx;
}

.canvas{
  position: absolute;
  left: 0;
  top: 0;
  background-color:transparent;
}