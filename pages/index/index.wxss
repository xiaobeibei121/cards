page{
  height: 100%;
  background-color: #f0f0f0;
  overflow: hidden;
}

.container{
  height: 100%;
  padding: 80rpx 0 30rpx;
  overflow: hidden;
}
.nav-banner{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 80rpx;
  background-color: #fff;
  z-index: 9;
}

.scroll-view_H{
  height: 80rpx;
  font-size: 0;
  overflow:hidden;
  white-space:nowrap;
}

::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}

.nav-title{
  padding-right:30rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: left;
  font-size: 28rpx;
  display: inline-block;
}

.nav-title:nth-child(1){
  padding-left: 30rpx;
}

.nav-title.active{
  font-weight: bold;
}

.time-view{
  position: relative;
  z-index: 2;
  height: 120rpx;
  line-height: 120rpx;
  background-color: #f0f0f0;
  font-size: 50rpx;
  padding-left: 30rpx;
  font-weight: bold;
  color: #666;
  padding-top: 80rpx;
}

.history-view{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 80rpx;
  padding: 0 30rpx;
  margin: 30rpx 0 0rpx;
  background-color: #fff;
}

.next-icon{
  width: 20rpx;
  height: 20rpx;
}

.no-data{
  background-color: #fff;
  height: 200rpx;
  text-align: center;
  font-size: 32rpx;
  color: #999;
  padding-top: 160rpx;
}

.scroll-list{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
}


.list-view {
  padding: 40rpx 30rpx;
  font-size: 0;
  background-color: #fff;
}

.list-banner {
  padding-top: 80rpx;
  margin-top: -80rpx;
  border-bottom: 1px solid #f0f0f0;
  margin-bottom: 40rpx;
}

.list-banner:last-child{
  border-bottom: none;
}

.list-banner .list-title{
  font-size: 32rpx;
  margin-bottom: 30rpx;
}

.list-banner .list-detail{
  margin-bottom: 30rpx;
  display: flex;
  justify-content: space-between;
}

.list-banner .list-text{
  font-size: 28rpx;
  width: 650rpx;
}

.dot{
  display: inline-block;
  width: 16rpx;
  height: 16rpx;
  background-color: #333;
  border-radius: 50%;
  margin-top: 10rpx;
}

.bottom-view{
  height: 30rpx;
}
